@model RI.UtilityApp.Models.TransactionLoadTestModel

@{
    ViewData["Title"] = "Index";
}
<main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4">
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Index">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="col-xs-12 col-sm-6 col-md-6 col-lg-4">
                    <label asp-for="PartnerId" class="control-label"></label>
                    <select asp-for="PartnerId" class="form-control" asp-items="@(Model.Partners)"></select>
                </div>
                <div class="form-group">
                    <input type="submit" value="Transaction" class="btn btn-default" />
                </div>
            </form>
        </div>
    </div>


    @section Scripts {
        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    }

</main>